<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.password_manager.Controllers.LoginController">
   <center>
      <VBox alignment="CENTER" prefHeight="182.0" prefWidth="633.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="labelText" alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="35.0" prefWidth="360.0" text="Wprowadz hasło do programu!" textAlignment="CENTER" wrapText="true" VBox.vgrow="NEVER">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <PasswordField fx:id="password" maxHeight="26.0" maxWidth="360.0" promptText="Hasło" VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </PasswordField>
            <Button fx:id="buttonLogIn" maxHeight="106.0" maxWidth="106.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="106.0" text="Zaloguj się" textAlignment="CENTER" VBox.vgrow="NEVER">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
